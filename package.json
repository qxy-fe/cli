{
  "name": "qxy",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@8.15.3",
  "description": "qxy cli",
  "keywords": [
    "cli"
  ],
  "license": "MIT",
  "homepage": "https://github.com/qxy-fe/cli#readme",
  "bugs": {
    "url": "https://github.com/qxy-fe/cli/issues"
  },
  "repository": "qxy-fe/cli",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "qxy": "./bin/qxy.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint build typecheck",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jimp/core": "^0.22.10",
    "@types/envinfo": "^7.8.3",
    "@types/fs-extra": "^11.0.4",
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "envinfo": "^7.11.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "jimp": "^0.22.10",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.20.3",
    "@ntnyq/prettier-config": "^1.20.3",
    "@types/node": "^20.11.19",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.1.2",
    "pnpm": "^8.15.3",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,mjs,ts,yml,yaml,json,md}": "eslint --fix"
  }
}
