{
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "esbuild",
    "esmodule",
    "netoa",
    "pnpm",
    "qixinyun",
    "tsbuildinfo",
    "tsup",
    "upath",
    "vitest"
  ]
}
